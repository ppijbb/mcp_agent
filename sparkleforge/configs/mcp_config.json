{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "env": {}
    },
    "github-local": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "local-search": {
      "command": "/home/user/miniconda3/envs/mcp/bin/python3",
      "args": ["/home/user/workspace/mcp_agent/local_mcp_servers/search_server.py"],
      "env": {
        "PYTHONPATH": "/home/user/workspace/mcp_agent"
      }
    },
    "code-executor": {
      "command": "/home/user/miniconda3/envs/mcp/bin/python3",
      "args": ["/home/user/workspace/mcp_agent/local_mcp_servers/code_executor.py"],
      "env": {
        "PYTHONPATH": "/home/user/workspace/mcp_agent"
      }
    },
    "data-analyzer": {
      "command": "/home/user/miniconda3/envs/mcp/bin/python3",
      "args": ["/home/user/workspace/mcp_agent/local_mcp_servers/data_analyzer.py"],
      "env": {
        "PYTHONPATH": "/home/user/workspace/mcp_agent"
      }
    }
  }
}