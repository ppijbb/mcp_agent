{
  "mcpServers": {
    "ddg_search": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@modelcontextprotocol/server-duckduckgo-search"
      ]
    },
    "fetch": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/fetch"
      ]
    },
    "docfork": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@docfork/mcp"
      ]
    },
    "context7-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@upstash/context7-mcp"
      ]
    },
    "exa": {
      "type": "http",
      "transport": "stdio",
      "httpUrl": "https://mcp.exa.ai/mcp",
      "headers": {
        "Remove-Me": "Disable web_search_exa tool if you're just coding. To 100% call exa-code, say 'use exa-code'."
      }
    },
    "semantic_scholar": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@hamid-vakilzadeh/mcpsemanticscholar"
      ]
    },
    "parallel-search": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@parallel/search"
      ]
    },
    "tavily-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@Jeetanshu18/tavily-mcp"
      ]
    },
    "WebSearch-MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@mnhlt/WebSearch-MCP"
      ]
    },
    "Google search console": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "google_search_console"
      ]
    },
    "Paper Search": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@adamamer20/paper-search-mcp-openai"
      ]
    },
    "TodoList": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "todoist"
      ]
    },
    "arxiv": {
      "command": "npx",
      "transport": "stdio",
      "args": [
        "-y",
        "@modelcontextprotocol/server-arxiv@latest"
      ]
    },
    "kis MCP": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@KISOpenAPI/kis-code-assistant-mcp"
      ]
    },
    "financial_agent": {
      "command": "python",
      "args": [
        "lang_graph/financial_agent/financial_mcp_server.py"
      ]
    }
  }
}

